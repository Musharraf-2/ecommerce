 <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <div class="container">
        <%= link_to 'Ecommerce', products_path, class: "navbar-brand" %>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <%= link_to 'Home', products_path, class: "nav-link" %>
            </li>
            <li class="nav-item">
              <%= link_to 'Cart', cart_path, class: "nav-link" %>
            </li>
            <li class="nav-item">
              <%= link_to 'Wishlist', wishlist_path, class: "nav-link" %>
            </li>
            <li class="nav-item">
               <%= link_to 'Product Dashboard', dashboard_products_path, class: "nav-link" %>
            </li>
            <% if user_signed_in? %>
              <li class="nav-item">
                <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "nav-link" %>
              </li>
              <li class="nav-item">
                <%= link_to current_user.email, edit_user_registration_path, class: "nav-link" %>
              </li>
              <li class="nav-item">
                <%= image_tag current_user.image, class: "rounded-circle profile-picture" %>
              </li>
            <% else %>
              <li class="nav-item">
                <%= link_to "Log In", new_user_session_path, class: "nav-link" %>
              </li>
              <li class="nav-item">
                <%= link_to "Sign up", new_user_registration_path, class: "nav-link" %>
              </li>
            <% end %>
          </ul>
          <%= form_with url: products_path, method: :get, local: true, class: "d-flex" do |form| %>
            <%= form.text_field :query, clas: "form-control" %>
            <%= form.submit "Search", class: "btn-success" %>
          <% end %>
        </div>
      </div>
    </nav>
    <br>
